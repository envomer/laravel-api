{
    "name": "envomer/laravel-api",
    "description": "One Class One API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Omer Mohamed Ali",
            "email": "om@envo.me",
            "homepage": "http://omer.pink",
            "role": "Developer"
        }
    ],
    "readme": "README.md",
    "require": {
        "illuminate/support": "^6.2",
        "php": "^7.2"
    },
    "require-dev": {
        "orchestra/testbench-dusk": "^4.2",
        "orchestra/testbench": "^4.2",
        "phpmd/phpmd": "^2.7",
        "squizlabs/php_codesniffer": "^3.5",
        "orchestra/testbench-browser-kit": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Envomer\\API\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Envomer\\API\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\Envomer\\API\\Providers\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
